; Modifying this file:
; You should not modify this file except to make bugfixes or
; for other "maintenance".  If you want to make custom changes,
; you should create a new datadir subdirectory and copy this file
; into that directory, and then modify that copy.  Then use the
; command "rulesetdir <mysubdir>" in the server to have freeciv
; use your new customized file.
;
; units.ruleset is generated from units.pre, so all changes should be
; made to units.pre, and then units.ruleset regenerated by running
; makeunits.py.  makeunits will sort units from worst to better.

[datafile]
description="Aviation unit_type data for Freeciv"
options="+Freeciv-ruleset-Devel-2017.Jan.02"
format_version=20

[control]
; Names for custom unit type flags. There can be up to 32 of these.
; name          = rule name; In some circumstances user may see this
;                 as part of some sentences, so try to make it descriptive
;                 and sensible.
; helptxt       = displayed in the help for units with this flag (optional)
flags =
  { "name", "helptxt"
    _("Airbase"), _("Can construct airfields and buoys.")
    _("TradeRoute"), _("Can establish trade routes (must travel to target city).")
    _("Merchant"), _("Can make single-shot trade (but not ongoing trade routes) by travelling to target city.")
    _("Recon"), _("Can investigate foreign cities (must travel to target city).")
    _("Embassy"), _("Can establish embassies in foreign cities (must travel to target city).")
    _("NoUpgrade"), _("This unit can't be upgraded when it becomes obsolete.")
    _("Cities")
    _("AddToCity")
    _("Bombarder"), _("Can bombard adjacent tiles.")
    _("Bombarder 2"), _("Can bombard cities and fortresses up to 2 tiles away.")
    _("Pillage")
    _("Cant_Fortify")
    _("CRscout")
    _("CRfighter")
    _("CRtf")
    _("CRattack")
    _("CRtb")
    _("CRlight")
    _("CRmedium")
    _("CRheavy")
    _("Missile"), _("Destroyed after attacking.")
    _("DiveBomber")
    _("AirAttacker"), _("Bad at attacking CAM.")
    _("BadDefender"), _("Bad at defending against ground troops.")
    _("BadAirDefender"), _("Bad at defending against bombers.")
    _("FatShip"), _("Bad at defending against submarines.")
    _("Sub")
    _("Sonar"), _("Can detect submarines from two tiles away.")
; Torpedo bombers with FP=2 should only have FP=1 against scrambling naval/
; floatplane fighters.  However, there's no way to get that with the game
; engine as it stands, so instead those fighters get double defence against
; units with this flag.
    _("LongTorp"), _("Bad at attacking naval fighters.  FirePower reduced to 1 when defending.")
    _("FieldRepair"), _("Repairs more rapidly on airfield extra.")
; These three are meant for use with actionenablers, but since we can't avoid
; excessive stackprotect in 3.0 (e.g. indestructible ground+air stacks), we
; use targets/Unreachable and so these are just documentation.  Hopefully in
; a later version we'll have http://www.hostedredmine.com/issues/873418 and
; be able to switch over to actionenablers, making these useful again.
; makeunits.py uses these flags to generate the targets= lines.
    _("AntiGround"), _("Can attack ground units (Land, Assault).")
    _("AntiSea"), _("Can attack Sea units.")
    _("AntiAir"), _("Can attack flying units (Air, Seaplane, Carrier-borne, Helicopter, Glider).")
    _("Jet"), _("Can only be built or repaired in a city with an Air Base.")
    _("Heavy"), _("Can only be repaired in a city with a Concrete Runway.")
    _("Nuclear")
    _("Capturer"),   _("Can capture enemy merchant ships (prizes).")
    _("Capturable"), _("Can be captured by some enemy units.")
    _("Spotter"), _("Can Spot targets for artillery or naval gunfire.")
    _("Firestarter"), _("Can drop incendiary bombs on cities to destroy production.")
  }

; Names for custom unit class flags. There can be up to 8 of these.
; name          = rule name; In some circumstances user may see this
;                 as part of some sentences, so try to make it descriptive
;                 and sensible.
; helptxt       = displayed in the help for unit types belonging to a class
;                 with this flag (optional)
class_flags =
  { "name", "helptxt"
    _("Ground")
    _("Seaborne")
    _("Flying")
    _("CanPillage")
    _("CanFortify"), _("Gets a 50% defensive bonus while in cities.")
  }

[veteran_system]
; What are the names of the levels?
veteran_names = _("green"), _("veteran"), _("ace"), _("elite")

; The percentage chance of increasing level through combat
veteran_base_raise_chance = 30, 15, 6, 0

; The percentage chance of a settler/engineer increasing level through
; performing useful work (per turn)
veteran_work_raise_chance = 0, 0, 0, 0

; Power factors are as a percentage.
; +50% is represented by 150
veteran_power_fact = 100, 150, 160, 200

; The additional number of movement points granted for different veteran
; levels. These are fractional move points as defined by move_fragments in
; terrain.ruleset.
veteran_move_bonus = 0, 0, 2, 2

; /* <-- avoid gettext warnings
;
; Unit classes
; The number can be variable, up to 32
; When adding new classes, remember to check effects.ruleset also.
; Eg. if you divide class 'Land' to two separate classes, you may
; want add effect giving City Walls defence bonus against new class
; too.
;
; The actual tag used (the * in [unitclass_*]) does not matter, except
; it must be unique within this file, and it may be used in debug
; output when reading this file.
;
; ** Fields **
;
; name                = translatable name as seen by user
; rule_name           = (optional) internal name for savegames, rulesets etc;
;                       if not present, "name" is used for this purpose too.
;                       Since the name used in savegames must not change, if
;                       you want to rename an item after a ruleset has been
;                       released, you should set "rule_name" to the original
;                       value of "name".
; min_speed           = Minimum speed after damage and effects (whole movement points)
; hp_loss_pct         = Hitpoints lost each turn if unit not in city or native base
; non_native_def_pct  = Defense power percentage applying when defending on
;                       non-native terrain (such as ship in harbour)
; hut_behavior        = What happens to huts when unit enters tile:
;                       "Normal", "Nothing", or "Frighten"
; flags               = List of unit class flags (from the following list; you
;                       cannot add custom unit class flags)
; helptext            = optional help text string; should escape all raw newlines
;                       so that xgettext parsing works
;
; ** Unit class Flags **
;
; "TerrainSpeed"   = Units use terrain specific speed
; "TerrainDefense" = Units gain defense bonus from terrain
; "DamageSlows"   = Damaged units are slowed down
; "CanOccupyCity" = Military units of this class can occupy enemy cities
; "BuildAnywhere" = Unit can be built even in the middle of non-native terrain
; "Unreachable"   = Unit can be attacked only by units explicitly listing this
;                   class in its 'targets', unless on a city or native base.
;                   For class members which are transports, cargo cannot load/
;                   unload except in a city or native base, unless that unit
;                   explicitly lists this class in its 'embarks'/'disembarks'.
; "CollectRansom" = Unit can collect ransom when killing lone barbarian leader
; "ZOC"           = Unit is subject to ZOC rules. Unit type flag "IgZOC" can
;                   override this
; "CanFortify"    = Unit can fortify at land tiles. Unit type flag
;                   "Cant_Fortify" can override this
; "CanPillage"    = Unit can pillage tile infrastructure
; "DoesntOccupyTile"  = Even if this kind of enemy unit is on tile, cities can
;                       still work that tile
; "AttackNonNative"   = Unit can attack units on non-native tiles. Unit type
;                       flag "Only_Native_Attack" can override this
; "AttFromNonNative"  = Unit can launch attack from non-native tile (against
;                       native one)
;                       This applies for both attacking from transport or
;                       cities. If only some unit types of the class should
;                       get this property, use type flag "Marines"
; "KillCitizen"       = Upon successful attack against a city, unit kills one
;                       citizen. The effect "Unit_No_Lose_Pop" and the server
;                       setting 'killcitizen' can disable this.
; "Airliftable"       = Unit may be airlifted
;
; */ <-- avoid gettext warnings

[unitclass_land]
; /* TRANS: Unit class: used adjectivally */
name          = _("?unitclass:Land")
min_speed     = 1
hp_loss_pct   = 0
flags         = "TerrainSpeed", "DamageSlows", "CanOccupyCity", "BuildAnywhere",
                "CollectRansom", "ZOC", "CanFortify", "CanPillage",
                "TerrainDefense", "KillCitizen", "Ground", "Unreachable"
helptext = _("Regular Land units are not transportable by air or in landing-craft, as their equipment (tanks and artillery) is too heavy.")

[unitclass_heavy_assault]
; /* TRANS: Unit class: used adjectivally */
name          = _("?unitclass:Heavy Assault")
min_speed     = 1
hp_loss_pct   = 0
flags         = "TerrainSpeed", "DamageSlows", "CanOccupyCity", "BuildAnywhere",
                "CollectRansom", "ZOC", "CanFortify", "CanPillage",
                "TerrainDefense", "KillCitizen", "Ground", "Unreachable"
helptext = _("Heavy Assault land units can be airlifted by strategic transport aircraft and heavy gliders.  They can also cross the seas in landing-craft.")

[unitclass_assault]
